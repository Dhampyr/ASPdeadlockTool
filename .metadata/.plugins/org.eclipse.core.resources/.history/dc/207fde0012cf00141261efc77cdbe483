package asp.models;

import java.util.HashMap;

public class Environment {
	
	private static HashMap<String,Declaration> environment;
	
	public Environment()
	{
		environment = new HashMap<>();
	}
	
	public static void putAll(HashMap<String,Declaration> set)
	{
		environment.putAll(set);
	}
	
	public static void putAllInt(HashMap<Integer,Declaration> set)
	{
		for(Integer key : set.keySet())
		{
			environment.put(set.get(key).getVar().getName(),set.get(key));
		}
	}

}
