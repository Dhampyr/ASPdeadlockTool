        call.setPosition(getStart(), getEnd());
        ASTNode parent = getParent();
        int i = parent.getIndexOfChild(this);
        parent.setChild(call, i);
        //System.out.println("*** replaced " + this + " with call to: " + originalMethodCopy.getMethodSig() + "\n");
    }
