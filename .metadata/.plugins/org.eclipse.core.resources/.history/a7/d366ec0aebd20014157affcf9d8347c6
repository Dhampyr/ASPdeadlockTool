  public void generateProlog(PrintStream s,ReachabilityInformation reachInfo){
        //generates "namedImport(ImportedDeclarationsList)."
        s.print("namedImport(");
        List<Name> importedDecl = this.getNameList();
        int n = importedDecl.getNumChild();
        for (int i = 0; i < n; i++){
